	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Zola",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/target/debug/zola.exe",
			"args": ["serve"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/../evan-brass.github.io/",
			"environment": [{
				"name": "RUST_BACKTRACE",
				"value": "1"
			}],
			"externalConsole": false
		}
	]
}